Proceso Inicio
	Escribir "Ingrese la distancia de los vehiculos"
	Leer d
	Escribir "Ingrese la velocidad (Km/h) del vehiculo 1"
	Leer V1
	Escribir "Ingrese la velocidad (Km/h) del vehiculo 2"
	Leer V2
	DifenciaVelocidades<-V1-V2
	T<-(d/DifenciaVelocidades)*(60)
	Escribir "Tiempo (min)= " T
FinProceso
